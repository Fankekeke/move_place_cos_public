<!--热销推荐-->
<view class="cu-bar bg-white solid-bottom">
    <view class='action'>
        <text class='icon-title text-orange'></text> 预约下单
    </view>
</view>
<view class="bg-white solid-bottom">
    <view style="padding: 15px 10px 20px 10px">
        <van-cell-group inset border="{{false}}">
            <van-cell clickable arrow-direction="left" border="{{false}}" center title="起始地点" label="{{ startPoint.startAddress }}" is-link bindtap="startChooseLocation"/>
            <van-cell clickable arrow-direction="left" border="{{false}}" center title="运输地点" label="{{ endPoint.endAddress }}" is-link bindtap="endChooseLocation"/>
        </van-cell-group>
        <view style="padding: 10px 12px 0 12px">
            <van-button type="info" size="normal" style="width: 100%" block @bindtap="submit">提交预约</van-button>
        </view>
    </view>
</view>

<!--宫格功能区域-->
<view class="cu-list margin-top grid col-4 no-border">
    <block wx:for="{{iconList}}" wx:key="item.id">
        <view class="cu-item" bindtap="itemckcred" data-index="{{item.id}}" data-itemtype="{{item.type}}">
            <view class='icon-{{item.icon}} text-{{item.color}}'> </view>
            <text>{{item.name}}</text>
        </view>
    </block>
</view>

